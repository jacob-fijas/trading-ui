{
  "name": "trader-ui",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron.js",
  "homepage": ".",
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "build",
    "start": "concurrently \"SET BROWSER=none&&react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "dev": "react-scripts start"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.8.3",
    "electron-is-dev": "^1.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-material-ui": "0.0.1",
    "react-scripts": "2.1.8",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "tslint": "^5.14.0",
    "tslint-react": "^3.6.0",
    "wait-on": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "electron-chess",
    "files": [
      "./build/**/*",
      "./public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
